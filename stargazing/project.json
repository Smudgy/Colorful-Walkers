{
  "file": "index.html",
  "contentrating": "Everyone",
  "title": "Stargazing",
  "description": "A audio reactive star field.",

  "type": "web",
  "preview": "prev.png",
  "monetization": false,
  "tags": ["Abstract"],
  "visibility": "public",
  "general":
  {
    "supportsaudioprocessing": true,
    "properties":
    {
      "amount":
      {
        "order": 1.00,
        "editable": true,

        "min": 1,
        "max": 800,
        "text": "Amount of stars",
        "type": "slider",
        "value": 300
      },
      "nrofBands":
      {
        "order": 1.1,
        "options": [
        {
          "label": "1",
          "value": 1
        },
        {
          "label": "2",
          "value": 2
        },
        {
          "label": "4",
          "value": 4
        },
        {
          "label": "8",
          "value": 8
        },
        {
          "label": "16",
          "value": 16
        },
        {
          "label": "32",
          "value": 32
        },
        {
          "label": "64",
          "value": 64
        }],
        "text": "Number of bands",
        "type": "combo",
        "value": 32
      },
      "bassRef":
      {
        "order": 1.2,

        "text": "Show bass reference? (top right)",
        "type": "bool",
        "value": false
      },
      "mouseTracking":
      {
        "order": 1.21,

        "text": "Let the stars follow the mouse?",
        "type": "bool",
        "value": true
      },
      "mouseVecAngle":
      {
        "order": 1.22,
        "editable": true,
        "condition": "!mouseTracking.value",

        "min": 0,
        "max": 360,
        "text": "Set the direction of the stars",
        "type": "slider",
        "value": 225
      }
    }
  }
}

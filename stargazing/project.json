{
    "file": "index.html",
    "contentrating": "Everyone",
    "title": "Stargazing",
    "description": "A audio reactive star field.",

    "type": "web",
    "preview": "prev.png",
    "monetization": false,
    "tags": ["Abstract"],
    "visibility": "public",
    "general":
    {
        "supportsaudioprocessing": true,
        "properties":
        {
            "amount":
            {
                "order": 1.00,
                "editable": true,

                "min": 1,
                "max": 800,
                "text": "Amount of stars",
                "type": "slider",
                "value": 300
            },
            "showBass":
            {
                "order": 1.1,

                "text": "Show the bass value meter?",
                "type": "bool",
                "value": false
            },
            "mouseTracking":
            {
                "order": 1.21,

                "text": "Let the stars follow the mouse?",
                "type": "bool",
                "value": true
            },
            "mouseVecAngle":
            {
                "order": 1.22,
                "editable": true,
                "condition": "!mouseTracking.value",

                "min": 0,
                "max": 360,
                "text": "Set the direction of the stars",
                "type": "slider",
                "value": 225
            }
        }
    }
}
